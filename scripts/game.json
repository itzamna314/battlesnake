        labels: [],
        datasets: datasets
      },
      options: {
        title: {
          display: false,
        },
        tooltips: {
          mode: 'index',
          intersect: false,
          xPadding: 10,
          yPadding: 10,
          caretPadding: 10,
          displayColors: false,
          itemSort: (a, b, data) => b.yLabel - a.yLabel
        },
        legend: {
          display: false
        },
        responsive: true,
        maintainAspectRatio: false,
        scales: {
          xAxes: [{
            display: false,
          }],
          yAxes: [{
            display: false,
            ticks: {
              beginAtZero: true,
              suggestedMax: 0  // This gets adjusted as we draw the chart
            }
          }]
        },
        elements: {
          line: {
            tension: 0
          },
          point: {
            radius: 0,
            borderWidth: 0
          }
        },
        layout: {
          padding: {
            left: 0,
            right: 0,
            top: 0,
            bottom: 0
          }
        },
        annotation: {
          annotations: [
            {
              type: "line",
              drawTime: "afterDatasetsDraw",
              mode: "vertical",
              scaleID: "x-axis-0",
              value: "", // Set by the frame
              borderColor: "black",
              borderWidth: 2,
            },
          ]
        }
      }
    };

    return new Chart(ctx, config);
  };

  var healthChart = createChart(document.getElementById("chart-health").getContext("2d"));
  var lengthChart = createChart(document.getElementById("chart-length").getContext("2d"));
  var latencyChart = createChart(document.getElementById("chart-latency").getContext("2d"));

  window.__battlesnake.hasShownGameOverModal = false;

  window.addEventListener('message', function (e) {
    if (e.origin !== __battlesnake.settings.BOARD_URL) {
      return;
    }

    var msg = e.data;

    if (msg.action === 'GAME_OVER') {
      if (!window.__battlesnake.hasShownGameOverModal) {
        window.__battlesnake.hasShownGameOverModal = true;
        fetch(window.location + 'end/')
        .then(function (response) {
          return response.json()
        }).then(function (data) {
          if (data.modal) {
            $('#gameOverModal').remove();
            $('body').append($(data.modal))
            $('#gameOverModal').modal('show');
          }
        });
      }

    } else if (msg.action === 'SET_CURRENT_FRAME') {
      // Update health chart
      if (msg.frame.turn > healthChart.data.labels.length) {
        healthChart.data.labels.push("Turn " + msg.frame.turn);
        healthChart.data.datasets.forEach((dataset) => {
          for (var i = 0; i < msg.frame.snakes.length; i++) {
            var snake = msg.frame.snakes[i];
            if (dataset.snakeID == snake.id) {
              if (!snake.isDead) {
                dataset.data.push(snake.health);
              } else {
                dataset.data.push(0);
              }
            }
          }
        });

        var maxY = 0;
        healthChart.data.datasets.forEach((dataset) => {
          maxY = Math.max(maxY, Math.max(...dataset.data));
        });
        healthChart.config.options.scales.yAxes[0].ticks.suggestedMax = maxY * 1.2;
      }
      healthChart.config.options.annotation.annotations[0].value = "Turn " + msg.frame.turn;
      healthChart.update();

      // Update length chart
      if (msg.frame.turn > lengthChart.data.labels.length) {
        lengthChart.data.labels.push("Turn " + msg.frame.turn);
        lengthChart.data.datasets.forEach((dataset) => {
          for (var i = 0; i < msg.frame.snakes.length; i++) {
            var snake = msg.frame.snakes[i];
            if (dataset.snakeID == snake.id) {
              if (!snake.isDead) {
                dataset.data.push(snake.body.length);
              } else {
                dataset.data.push(0);
              }
            }
          }
        });

        var maxY = 0;
        lengthChart.data.datasets.forEach((dataset) => {
          maxY = Math.max(maxY, Math.max(...dataset.data));
        });
        lengthChart.config.options.scales.yAxes[0].ticks.suggestedMax = maxY * 1.2;
      }
      lengthChart.config.options.annotation.annotations[0].value = "Turn " + msg.frame.turn;
      lengthChart.update();

      // Update latency chart
      if (msg.frame.turn > latencyChart.data.labels.length) {
        latencyChart.data.labels.push("Turn " + msg.frame.turn);
        latencyChart.data.datasets.forEach((dataset) => {
          for (var i = 0; i < msg.frame.snakes.length; i++) {
            var snake = msg.frame.snakes[i];
            if (dataset.snakeID == snake.id) {
              if (!snake.isDead) {
                var latency = parseInt(snake.latency);
                if (latency == 0) {
                  latency = 500;
                }
                dataset.data.push(latency);
              } else {
                dataset.data.push(0);
              }
            }
          }
        });

        var maxY = 0;
        latencyChart.data.datasets.forEach((dataset) => {
          maxY = Math.max(maxY, Math.max(...dataset.data));
        });
        latencyChart.config.options.scales.yAxes[0].ticks.suggestedMax = maxY * 1.2;
      }
      latencyChart.config.options.annotation.annotations[0].value = "Turn " + msg.frame.turn;
      latencyChart.update();
    }
  });
});
</script>

<div class="modal fade" id="arenaMatchMakingModal" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered"" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <p class="text-primary text-center"><strong><span class="status-text" style="text-transform: capitalize;"></span></strong></p>
        <ul class="list-group list-group-flush borderless battlesnakes"></ul>
        <div class="spinner text-center">
          <p class="spinner-border text-primary" role="status">
            <span class="sr-only">Loading...</span>
          </p>
        </div>
      </div>
    </div>
  </div>
</div>

<script>
  $(document).ready(function () {

    function addOpponentToModal(battlesnake) {
      $('#arenaMatchMakingModal ul.battlesnakes').append($(
        '<li class="text-center list-group-item border-0">' + battlesnake.name + '<br> <small>by ' + battlesnake.author + '</small></li>'
      ));
    }

    $('#arenaMatchMakingModal').on('show.bs.modal', function (e) {
      var gameMode = $(e.relatedTarget).data('game-mode');
      $('#arenaMatchMakingModal .status-text').text('Building ' + gameMode + ' Bhumba...');
    });

    $('#arenaMatchMakingModal').on('shown.bs.modal', function (e) {
      var leaderboardSnakeID = $(e.relatedTarget).data('leaderboard-snake-id');
      var gameMode = $(e.relatedTarget).data('game-mode');

      $.ajax({
        url: "/arena/play/" + leaderboardSnakeID + "/" + gameMode + "/",
        type: "POST",
        success: function (resp) {
          for (var i = 0; i < resp.battlesnakes.length; i++) {
            var timeout = (i * 750) + Math.floor(Math.random() * 250);
            setTimeout(addOpponentToModal, timeout, resp.battlesnakes[i]);
            if (i == resp.battlesnakes.length - 1) {
              setTimeout(function () {
                $('#arenaMatchMakingModal div.spinner').remove();
                $('#arenaMatchMakingModal .status-text').addClass('text-accent').text('Loading Game...');
              }, timeout);
              setTimeout(function () {
                window.location = resp.gameURL;
              }, timeout + 1500);
            }
          }
        },
        error: function (resp) {
          alert(resp.responseText);
          $('#arenaMatchMakingModal').modal('hide');
        }
      });
    });
  });
</script>



</body>

</html>

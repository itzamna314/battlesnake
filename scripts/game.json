  <style>
  /***
   * bvanvugt: these are pretty heavy theme changes, grouped together on purpose.
   * The comment values represent the original theme.
   */
  body {
    /* background: #f0f6ff!important; */
    background: #F4F3F9 !important;
  }

  /* Bootstrap 4.5 changed this, revert back to what the theme wants */
  body .table {
    color: #617182;
  }

  /* Make icons work in secondary buttons */
  body a.btn-secondary:visited,
  body a.btn-secondary:link,
  body a.btn-secondary i {
    color: #788db4;
  }

  body a.btn-secondary:hover,
  body a.btn-secondary:hover i {
    color: #ffffff;
  }

  body a.btn-secondary i {
    transition-duration: 0.2s;
  }

  body .btn, body a.btn, body a:link.btn, body button.btn {
    text-transform: uppercase;
    font-weight: 500;
  }

  .card {
    /* box-shadow: 0 10px 40px 0 rgba(18,106,211,.07), 0 2px 9px 0 rgba(18,106,211,.06); */
    box-shadow: 0 10px 40px 0 rgba(165, 178, 193, 0.4), 0 2px 9px 0 rgba(195, 195, 210, 0.5);

  }

  .card.card-elevate:hover {
    /* box-shadow: 0 16px 32px 0 rgba(100,143,195,.28), 0 0 0 transparent; */
    box-shadow: 0 16px 32px 0 rgba(165, 178, 193, .5), 0 0 0 transparent;
  }

  .table-striped tbody tr:nth-of-type(odd) {
    /* background-color: #f0f6ff; */
    background-color: #F4F3F9;
  }

  /* Navbar */
  body.layout-horizontal .header-wrapper {
    height: 100px;
  }
  @media (max-width: 992px) {
    body.layout-horizontal .header-wrapper {
      height: 50px;
    }
  }

  body.layout-horizontal .content-wrapper {
    padding: 20px 35px;
  }
  @media (max-width: 992px) {
    body.layout-horizontal .content-wrapper {
      margin-top: 50px;
      padding: 20px 15px;
    }
  }
  body.layout-horizontal .content-wrapper .content {
    margin-top: 0;
  }
  /* Navbar - Top */
  body .header-wrapper .header-top {
    /* background: linear-gradient(135deg,#534ca7 21%,#3190f0 100%,#3190f0 100%); */
    background: linear-gradient(135deg, #E80987 15%, #3E338F 100%);
    height: 50px;
    padding: 10px;
  }
  body .header-wrapper .header-top .top-toolbar {
    top: 0;
  }
  body .navbar-site-logo img {
    height: 30px;
  }
  body .navbar-site-logo {
    display: flex;
    justify-content: center;
  }
  @media (min-width: 992px) {
    body .navbar-site-logo {
      justify-content: start;
    }
  }
  @media (max-width: 992px) {
    body .header-wrapper .mobile-only li {
      line-height: 1.5;
      height: 30px;
    }
    body .header-wrapper .mobile-only li a {
      height: 30px;
    }
    body .header-wrapper .mobile-only li a i {
      font-size: 1.5rem;
      height: 30px;
      width: 30px;
    }
  }
  body .header-wrapper .header-top .top-toolbar ul li.nav-item>a {
    color: #F0F0F0;
    padding: 5px 10px;
  }
  body .header-wrapper .header-top .top-toolbar ul li.nav-item>a>i,
  body .header-wrapper .header-top .top-toolbar ul li.nav-item>a>i:hover {
    color: #F0F0F0;
  }
  body .top-toolbar-wrapper .top-toolbar .navbar-nav .nav-item>a:hover i.icon:after {
    position: absolute;
    top: -8px;
    left: -7px;
  }
  @media (max-width: 992px) {
    body .header-wrapper .header-top .top-toolbar ul li.nav-item>a,
    body .header-wrapper .header-top .top-toolbar ul li.nav-item>a>i,
    body .header-wrapper .header-top .top-toolbar ul li.nav-item>a>i:hover {
      color: #788db4;
      font-size: 1.125rem;
    }
  }
  /* Navbar - Bottom */
  body.menu-auto-hide .header-wrapper .header-bottom {
    top: 50px;
    height: 50px;
  }
  body .header-wrapper .header-bottom .main-menu {
    padding: 0;
  }
  @media (max-width: 992px) {
    body.menu-auto-hide .header-wrapper .header-bottom {
      height: 100vh;
      top: 0;
    }
  }

  /* footer */
  @media (max-width: 992px) {
    body .container.footer {
      padding-left: 30px;
      padding-right: 30px;
    }
  }

  /* reduce h1 size on mobile */
  @media (max-width: 992px) {
    body h1.display-4 {
      font-size: 2.5rem;
    }
  }

  /**
   * Different color logo for the sign in form.
  */
  .sign-in-form .battlesnake-logo {
    fill: #6f42c1;
  }

  /**
   * Hack for modals inside list-group-items
   * https://github.com/twbs/bootstrap/issues/25206
   */
  .list-group-item,
  .list-group-item:hover {
    z-index: auto !important;
  }

  /* Fix for theme btn CSS*/
  a:link.btn.btn-sm {
    line-height: 1.6;
  }

  /* Fix for popover widths being too restrictive in the theme */
  .popover {
    min-width: 280px;
    max-width: 400px;
  }

  /* Select2 Tweaks */
  .select2-container--default .select2-selection--multiple .select2-selection__choice,
  .select2-container--default.select2-container--disabled .select2-selection--single {
    margin-top: 7px;
  }

  /**
   * SVG logo
   */

  .logo a {
    display: block;
  }

  .battlesnake-logo {
    fill: #fff;
    width: 100px;
  }

  @media (min-width: 992px) {
    .battlesnake-logo {
      width: 140px;
    }
  }

  .sign-in-form .battlesnake-logo {
    width: 240px;
  }

  /**
   * Homepage tournament media banner
   */
  .tournament-media-play-link {
    height: 140px;
  }

  @media (min-width: 576px) {
    .tournament-media-play-link {
      height: 200px;
    }
  }

  @media (min-width: 768px) {
    .tournament-media-play-link {
      height: 300px;
    }
  }

  @media (min-width: 992px) {
    .tournament-media-play-link {
      height: 140px;
    }
  }

  @media (min-width: 1200px) {
    .tournament-media-play-link {
      height: 200px;
    }
  }

  /**
  * Table Highlights. These did not come with the theme.
  */
  table .table-primary,
  table .table-primary>td,
  table .table-primary>th {
    color: #ffffff;
    background-color: #635ebe;
  }

  table .table-secondary,
  table .table-secondary>td,
  table .table-secondary>th {
    color: #ffffff;
    background-color: #788db4;
  }

  table .table-accent,
  table .table-accent>td,
  table .table-accent>th {
    color: #ffffff;
    background-color: #f64a91;
  }


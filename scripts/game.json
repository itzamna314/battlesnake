<script>
  var battlesnake = {}

  /**
   * ClipboardJS integration on buttons
   */
  $('[data-clipboard-text]').tooltip({
    trigger: 'click',
    placement: 'top'
  })

  battlesnake.setTooltip = function (triggerElem, message) {
    $(triggerElem)
      .tooltip('dispose')
      .attr('data-original-title', message)
      .tooltip('show')
  }

  battlesnake.hideTooltip = function () {
    setTimeout(function () {
      // Blanket-hide all
      $('[data-clipboard-text]').tooltip('dispose')
    }, 1000)
  }

  var clipboard = new ClipboardJS('[data-clipboard-text]')

  clipboard.on('success', function (event) {
    battlesnake.setTooltip(event.trigger, 'Copied!')
    battlesnake.hideTooltip()
  })

  clipboard.on('error', function (event) {
    battlesnake.setTooltip('Failed to copy!')
    battlesnake.hideTooltip()
  })

  battlesnake.settings = {
    BOARD_URL: 'https://board.battlesnake.com',
    ENGINE_URL: 'https://engine.battlesnake.com'
  }

  function csrfSafeMethod(method) {
    // these HTTP methods do not require CSRF protection
    return (/^(GET|HEAD|OPTIONS|TRACE)$/.test(method))
  }

  $.ajaxSetup({
    beforeSend: function (xhr, settings) {
      if (!csrfSafeMethod(settings.type) && !this.crossDomain) {
        xhr.setRequestHeader("X-CSRFToken", 'NQrwwyk3UfMcOZesjjXVp76lzuiSaZlw0weTtbstICitNewHdbDINr9stvT38TVB')
      }
    }
  })

  window.__battlesnake = battlesnake;

  /**
   * Disable form submits after click to prevent double submits
   */
   (function($) {
    $('form[method=post]').submit(function(e) {
      $(e.target).find("button[type=submit]").each(function() {
        $(this).prop('disabled', true);
      });
    });
   })(jQuery);

  /**
   * Localize times and timezones.
   * .tz-display --> displays browser timezone, ie "America/Vancouver"
   * .tz-localize --> converts ISO UTC into local time and formats
   */
  (function($) {
    var targetTZ = jstz.determine() || 'UTC';
    $('.tz-display').text(targetTZ.name());
    $('.tz-localize').each(function(i) {
      var localized = moment($(this).data('tz-src'), moment.ISO_8601).tz(targetTZ.name())
      $(this).text(localized.format($(this).data('tz-dst')));
    });
  })(jQuery);

  /**
   * jquery.countUp
   * @author hisayoshi hayashi: HYS INC.
   * @license MIT license
   *
   */
  (function($) {
    $.fn.countUp = function(options) {
      var start_num = Number($(this).text());
      var d = {
        start: 0,
        last: 100,
        duration: 1000,
        frame: 33,
        update: null,
        complete: null
      };
      var o = $.extend(d, options);
      var $that = $(this);

      o.last = parseInt(o.last);
      o.duration = parseInt(o.duration);
      o.frame = parseInt(o.frame);

      $that.each(function(i) {
        var $t = $(this);
        var total_frame = 0;
        var split = (o.last - o.start) / (o.duration / 33);
        var value = o.start;

        var clear = setInterval(function() {
          value = Math.round(value + split);
          $t.text(value);

          if (typeof o.update == 'function') {
            o.update.call($t, value);
          }

          if (total_frame > o.duration) {
            clearInterval(clear);
            $t.text(o.last);

            if (typeof o.complete == 'function') {
              o.complete.call($t, value);
            }
          }
          total_frame += o.frame;
        }, o.frame);
      });
      return $that;
    };
  })(jQuery);
</script>


